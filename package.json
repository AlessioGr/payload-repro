{
  "name": "payload-template-ts-blank",
  "description": "Payload project created from ts-blank template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.223.0",
    "@lexical/clipboard": "0.6.4",
    "@lexical/code": "0.6.4",
    "@lexical/file": "0.6.4",
    "@lexical/hashtag": "0.6.4",
    "@lexical/headless": "0.6.4",
    "@lexical/html": "0.6.4",
    "@lexical/link": "0.6.4",
    "@lexical/list": "0.6.4",
    "@lexical/mark": "0.6.4",
    "@lexical/markdown": "0.6.4",
    "@lexical/overflow": "0.6.4",
    "@lexical/plain-text": "0.6.4",
    "@lexical/react": "0.6.4",
    "@lexical/rich-text": "0.6.4",
    "@lexical/selection": "0.6.4",
    "@lexical/table": "0.6.4",
    "@lexical/utils": "0.6.4",
    "@lexical/yjs": "0.6.4",
    "@payloadcms/plugin-cloud-storage": "^1.0.12",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "katex": "^0.16.3",
    "lexical": "0.6.4",
    "lodash": "^4.17.21",
    "payload": "^1.2.4",
    "yjs": "^13.5.43"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  }
}
